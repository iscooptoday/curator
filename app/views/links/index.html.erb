<h1>Listing links</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Url</th>
      <th>Topic</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.description %></td>
        <td><%= link.url %></td>
        <td><%= link.url2 %></td>
        <td><%= link.url3 %></td>
        <td><%= link.url4 %></td>
        <td><%= link.url5 %></td>
        <td><%= link.topic.description %></td>
        <td><%= link_to 'Show', link %></td>
        <td><%= link_to 'Edit', edit_link_path(link) %></td>
        <td><%= link_to 'Destroy', link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


